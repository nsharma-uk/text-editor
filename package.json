{
  "name": "text-editor",
  "version": "1.0.0",
  "description": "A text editor which runs in the browser and uses an IndexedDB database.",
  "main": "index.js",
  "scripts": {
    "start:dev": "concurrently \"cd server && npm run server\" \"cd client && npm run dev\"",
    "start": "npm run build && cd server && node server.js",
    "server": "cd server nodemon server.js --ignore client",
    "build": "cd client && npm run build",
    "install": "cd client && npm install",
    "client": "cd client && npm start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nsharma-uk/text-editor.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nsharma-uk/text-editor/issues"
  },
  "homepage": "https://github.com/nsharma-uk/text-editor#readme",
  "dependencies": {
    "node": "^16.17.0"
  }
}
